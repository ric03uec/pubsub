{
  "name": "pubsub",
  "version": "1.0.0",
  "private": false,
  "main": "api.app.js",
  "scripts": {
    "start": "node subscribe.js"
  },
  "dependencies": {
    "amqp": "0.2.6"
  },
  "jshintConfig": {
    "node": true,
    "undef": true,
    "noarg": true,
    "white": true,
    "esnext": true,
    "strict": true,
    "newcap": true,
    "nonbsp": true,
    "eqeqeq": true,
    "unused": true,
    "maxlen": 80,
    "indent": 2,
    "bitwise": true,
    "noempty": true,
    "latedef": "nofunc",
    "maxdepth": 5,
    "quotmark": "single",
    "trailing": true,
    "camelcase": true,
    "maxparams": 8,
    "globalstrict": true,
    "maxcomplexity": 10,
    "maxstatements": 40
  }
}
